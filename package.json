{
  "name": "cooking-contest",
  "description": "a Svelte-powered Progressive Web App for cooking contests, relying on Firebase for auth and data storage",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/doppelganger9/cooking-contest.git"
  },
  "license": "MIT",
  "keywords": [
    "svelte",
    "sveltejs",
    "cooking",
    "contest",
    "pwa",
    "firebase"
  ],
  "devDependencies": {
    "cypress": "4.9.0",
    "dotenv": "6.2.0",
    "firebase": "7.15.5",
    "firebase-admin": "7.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "1.25.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "6.1.0",
    "rxfire": "3.4.6",
    "rxjs": "6.4.0",
    "sirv-cli": "^0.3.1",
    "start-server-and-test": "1.10.6"
  },
  "engines": {
    "node": "=10"
  },
  "dependencies": {
    "spectre.css": "0.5.8",
    "svelte": "3.23.2"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "test:e2e": "start-server-and-test start http://localhost:5000 cy:run",
    "cy:run": "cypress run",
    "cy:open": "cypress open"
