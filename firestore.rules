service cloud.firestore {
  match /databases/{database}/documents {
    match /contests/{contest} {
    	allow read;
    }
    match /meals/{meal} {
    	allow read;
    	allow write: if request.auth.uid != null;
    }
    match /evaluations/{evaluation} {
    	allow read;
    	allow write: if request.auth.uid != null;
    }
  }
}