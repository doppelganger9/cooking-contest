{
  "name": "cooking-contest",
  "description": "a Svelte-powered Progressive Web App for cooking contests, relying on Firebase for auth and data storage",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/doppelganger9/cooking-contest.git"
  },
  "license": "MIT",
  "keywords": [
    "svelte",
    "sveltejs",
    "cooking",
    "contest",
    "pwa",
    "firebase"
  ],
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@cypress/code-coverage": "1.10.2",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "cypress": "4.9.0",
    "babelrc-rollup": "^3.0.0",
    "dotenv": "6.2.0",
    "firebase": "7.24.0",
    "firebase-admin": "7.0.0",
    "istanbul-lib-coverage": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "nyc": "14.1.1",
    "rollup": "2.18.1",
    "rollup-plugin-istanbul": "git+https://github.com/billowz/rollup-plugin-istanbul.git#rc",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "6.1.0",
    "rxfire": "3.4.6",
    "rxjs": "6.4.0",
    "sirv-cli": "^0.3.1",
    "source-map-support": "0.5.19",
    "start-server-and-test": "1.10.6"
  },
  "engines": {
    "node": "=10"
  },
  "dependencies": {
    "spectre.css": "0.5.8",
    "svelte": "3.23.2"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --single --dev",
    "e2e": "start-test dev 5000 cy:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "coverage:report": "nyc report --reporter=lcov"
  },
  "nyc": {
    "cache": false,
    "extension": [
      ".js",
      ".svelte"
    ],
    "all": true
  }
}
